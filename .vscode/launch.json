{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch REPL",
      "preLaunchTask": "npm: build",
      "program": "${workspaceRoot}/dist/index.js",
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Jest",
      "preLaunchTask": "npm: build",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "console": "integratedTerminal"
    }
  ]
}
