{
  "defaultSeverity": "error",
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "member-access": [
      true,
      "check-accessor",
      "check-constructor"
    ],
    "object-literal-sort-keys": false,
    "typedef": [
      true,
      "call-signature",
      "arrow-call-signature",
      "parameter",
      "arrow-parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration",
      "object-destructuring",
      "array-destructuring"
    ]
  }
}
