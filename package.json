{
  "name": "squirrel",
  "version": "0.0.1",
  "description": "An expression-oriented programming language inspired by Lisp",
  "license": "MIT",
  "author": "Joshua Escamilla <joshua.escamilla@selu.edu>",
  "repository": "https://github.com/escamilla/squirrel.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "eval": "npm run build && node dist/main.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.0.33",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
