{
  "name": "squirrel",
  "version": "0.0.1",
  "description": "An expression-oriented programming language inspired by Lisp",
  "license": "MIT",
  "author": "Joshua Escamilla <joshua.escamilla@selu.edu>",
  "repository": "https://github.com/escamilla/squirrel.ts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "tslint -p tsconfig.json",
    "eval": "npm run build && node dist/main.js"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/minimist": "^1.2.0",
    "@types/node": "^9.4.0",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
