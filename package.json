      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
