{
  "name": "squirrel-cli",
  "author": "Joshua Escamilla <joshua.escamilla@selu.edu>",
  "version": "0.2.0",
  "license": "MIT",
  "description": "A command line interface for running Squirrel programs",
  "keywords": [
    "javascript",
    "language",
    "lisp",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/escamilla/squirrel-cli.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "start": "npm run build && node dist/index.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "readline-sync": "^1.4.9",
    "squirrel-core": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.7",
    "@types/readline-sync": "^1.4.3",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
